<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.peterstolcz.musicalstructureapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    <activity
        android:name=".MyMusicActivity"
        android:label="@string/mainmenu1" />
    <activity
        android:name=".GenreActivity"
        android:label="@string/mainmenu2" />
    <activity
        android:name=".ArtistActivity"
        android:label="@string/mainmenu3" />
    <activity
        android:name=".RadioActivity"
        android:label="@string/mainmenu4"/>
    <activity
        android:name=".NowisplayingActivity"
        android:label="@string/nowplaying"/>
</application>


</manifest>